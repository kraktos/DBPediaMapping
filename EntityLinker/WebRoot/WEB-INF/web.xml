<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>
	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>LinkEntity</display-name>
		<servlet-name>EntryServlet</servlet-name>
		<servlet-class>de.dws.mapper.webInterface.servlet.EntryServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EntryServlet</servlet-name>
		<url-pattern>/EntryServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>page/entry.jsp</welcome-file>
	</welcome-file-list>
</web-app>
